{
    "name": "ynn-dreamerv3-torch",
    "image": "dreamerv3-torch",
    "runArgs": [
        "--net=host",
        "--ipc=host",
        "--gpus",
        "all",
        "--privileged"
    ],
    "mounts": [
        "source=/home/ynn/code/search/dreamerv3-torch,target=/home/root/dreamerv3-torch,type=bind",
        "source=/mnt/data1/ynn/expertdata,target=/expertdata,type=bind"
    ],
    "workspaceFolder": "/home/root/dreamerv3-torch",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance",
                "ms-python.black-formatter",
                "ms-azuretools.vscode-docker",
                "github.copilot",
                "github.vscode-pull-request-github"
            ]
        }
    }
}